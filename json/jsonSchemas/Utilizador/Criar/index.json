{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Product",
    "description": "A product from Acme's catalog",
    "type": "object",
    "properties": 
    {
        "Utilizador": 
        {
            "type": "object",
            "properties": 
            {
                "Email": 
                {
                    "type": "string",
                    "pattern":"[^@ \t\r\n]+@[^@ \t\r\n]+/.[^@ \t\r\n]+"
                },
                "Password": 
                {
                    "type": "string"
                },
                "Nome": 
                {
                    "type": "string"
                },
                "Apelido": 
                {
                    "type": "string"
                },
                "Contacto": 
                {
                    "type": "string",
                    "pattern": ""
                }
                
            }
        }
    }
}
    