{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": 
    {
        "Utilizador": 
        {
            "type": "object",
            "properties": 
            {
                "Email": 
                {
                    "type": "string",
                    "pattern":"[^@ \t\r\n]+@[^@ \t\r\n]+/.[^@ \t\r\n]+"
                },
                "Password": 
                {
                    "type": "string"
                },
                "Nome": 
                {
                    "type": "string"
                },
                "Apelido": 
                {
                    "type": "string"
                },
                "Contacto": 
                {
                    "type": "string",
                    "pattern": "^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{3}$"
                }
            }
        }
    }
}
    